    RowBox[{
     RowBox[{"R", "[", "Rep", "]"}], " ", "=", " ", 
     RowBox[{"RTable", "[", 
      RowBox[{"DColor", ",", "Phi", ",", "Psi"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
