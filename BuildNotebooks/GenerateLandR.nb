 RowBox[{
  RowBox[{"GenerateLandR", "[", 
   RowBox[{"DColor_", ",", "Phi_", ",", "Psi_", ",", "Rep_"}], "]"}], ":=", 
  " ", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"L", "[", "Rep", "]"}], " ", "=", " ", 
     RowBox[{"LTable", "[", 
